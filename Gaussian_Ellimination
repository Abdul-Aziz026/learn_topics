#include <bits/stdc++.h>
using namespace std;

//debug..........
#ifdef Azizul
#include "debug.cpp"
#else
#define dbg(x...)
#endif


#define int long long
#define Yes cout << "Yes\n"
#define No cout << "No\n"
double a[100][100], val[100] ;
int n ;

// back substitution...
double cal(int r, int c) {
	double res = a[r][n];
	// dbg(r, c, res)
	for (int j=c+1;j<n;++j) {
		res -= a[r][j] * val[j] ;
	}
	res /= a[r][c] ;
	return res;
}


void change(int r1, int r2, double div, double mul, int sn) {
	for (int j=0;j<n+1;++j) {
		double num = a[r1][j] / div * mul ;
		a[r2][j] += sn * num ;
	}
}

signed main() {
    ios_base::sync_with_stdio(0);
    cin.tie(0); 

    cin >> n ;
    for (int i=0;i<n;++i) {
    	for (int j=0;j<n+1;++j) {
    		cin >> a[i][j] ;
    	}
    }
    for (int i=0;i<n;++i) {
    	for (int j=0;j<n;++j) {
    		if ((i <= j) or a[i][j] == 0) continue ;
    		int r1 = j, r2 = i ;
    		if ((a[r1][j] > 0 && a[r2][j] < 0) or (a[r1][j] < 0 && a[r2][j] > 0)) {
    			// r2 += (a[r1][j] / a[r1][j] * a[r2][j])
    			change(r1, r2, a[r1][j], a[r2][j], 1) ;
    		}
    		else {
    			change(r1, r2, a[r1][j], a[r2][j], -1) ;
	    	}
    	}
	    
    }
    cout << n << endl;
    for (int i=n-1;i>=0;--i) {
    	val[i] = cal(i, i) ;
    	cout << fixed << setprecision(5) ;
    	cout << i+1 << " = " << val[i] << endl;
    }
    return 0;
}
